{
  "name": "vuillaume-agency/symfony-turnstile",
  "description": "A Symfony (>= 7.4) bundle to integrate Cloudflare Turnstile on forms.",
  "keywords": [
    "symfony",
    "bundle",
    "cloudflare",
    "turnstile",
    "form",
    "security"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Vuillaume Agency",
      "homepage": "https://www.vuillaume-agency.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "symfony/form": "^7.4|^8.0",
    "symfony/framework-bundle": "^7.4|^8.0",
    "symfony/http-client": "^7.4|^8.0",
    "symfony/yaml": "^7.4|^8.0",
    "symfony/validator": "^7.4|^8.0",
    "symfony/twig-bundle": "^7.4|^8.0"
  },
  "autoload": {
    "psr-4": {
      "VuillaumeAgency\\TurnstileBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VuillaumeAgency\\TurnstileBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.65",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-symfony": "^2.0",
    "phpunit/phpunit": "^12.0",
    "rector/rector": "^2.0"
  }
}
